{
  "name": "sams-suit-shop-backend",
  "version": "1.0.0",
  "description": "Backend for Sam's Suit Shop",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --import tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "seed": "node --import tsx src/db/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^9.2.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "@aws-sdk/client-s3": "^3.529.0",
    "@aws-sdk/client-cloudfront": "^3.529.0",
    "@aws-sdk/s3-request-presigner": "^3.529.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.1.1",
    "@vitest/ui": "^1.1.1",
    "@vitest/coverage-v8": "^1.1.1"
  }
}